        .include MACROS.inc

        .org 0

Z:          .data 0 
PLUS1:      .data 1 
MINUS1:     .data (-1)
        JMP Start

S:          .data $+1, "MATS",0

S1:         .data $+1, "OLLE",0
S2:         .data $+1, "MATS",0
S3:         .data $+1, "ENGSTROM",0
S4:         .data $+1, "Z",0
S5:         .data $+1, "",0  

Start:
        MOV stack_ SP                              ; Initialize the stack pointer

        PUSH S
        PUSH S1
        CALL CompareString   
        PUSH compResult
        CALL PrintInteger

        PUSH S
        PUSH S2
        CALL CompareString   
        PUSH compResult
        CALL PrintInteger

        PUSH S
        PUSH S3
        CALL CompareString   
        PUSH compResult
        CALL PrintInteger

        PUSH S
        PUSH S4
        CALL CompareString   
        PUSH compResult
        CALL PrintInteger

        PUSH S
        PUSH S5
        CALL CompareString   
        PUSH compResult
        CALL PrintInteger

        PRINTCRLF
        HALT

compResult:        .data 0

A:          .data 0              ; test variable
B:          .data 0              ; test variable
T:          .data 0
tmpA:       .data 0             ;Used by macros
tmpB:       .data 0             ;Used by macros
tmpC:       .data 0             ;Used by macros


        .include IO.inc
        .include STRING.inc
        .include CONSTANTS.inc
        .include STACK.inc
